# Environment variables for Firebase Functions (local and deployed)
# Access in code with process.env.NAME

# Firebase
FIREBASE_PROJECT_ID=

# Functions runtime
FUNCTIONS_REGION=us-central1
NODE_ENV=development
LOG_LEVEL=info

# Emulator settings (local only)
FUNCTIONS_EMULATOR=true
EMULATORS_HOST=localhost
AUTH_EMULATOR_PORT=9099
FIRESTORE_EMULATOR_PORT=8080
STORAGE_EMULATOR_PORT=9199
FUNCTIONS_EMULATOR_PORT=5001